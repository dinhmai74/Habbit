{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "skipDefaultLibCheck": true,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules","babel.config.js"]
}
